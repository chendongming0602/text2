@import '/utils/css/all.wxss';
.tabBox{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 96rpx;
  background:#fff;
  display: flex;
  justify-content: space-around;
  align-items: center;
  box-shadow:0px -4rpx 8rpx 0rpx rgba(0,0,0,0.11);
  z-index: 99;
}
.tabList{
  text-align: center;
  font-size: 24rpx;
  flex-grow: 1;
  color:#2AA2D0;
}
.tabImg{
  width: 48rpx;
  height: 48rpx;
  margin: 0 auto;
}
.tabListColor{
  color:rgba(191,191,191,1);
}